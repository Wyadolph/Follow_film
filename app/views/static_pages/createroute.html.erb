	<h1>Show flim tracks</h1>
	<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=true"></script>
	<%= javascript_include_tag "application"%>
	<script type="text/javascript">
		google.maps.event.addDomListener(window, 'load', initialize);
    </script>
    <body>
		<div id="map-canvas" style="width:760px;height:600px"></div>
		<!--c -->
		<%= form_tag action: "search" do %>
			<%= text_field_tag(:location) %>
			<!-- <div placeholder=".input-xxlarge"> -->
			<%= select_tag(:searchId, options_for_select([['film',1],['location',2]])) %>
			<%= submit_tag 'Search', :class => "btn btn-primary" %>
			<!-- </div> -->

		<% end %>
		<%= form_tag action: "addRoute" do %>
			<%= text_field_tag(:addroute)%>
			<%= submit_tag 'add a location of route', :class => "btn btn-primary" %>
		<% end %>
		<%= form_tag action: "newRoute" do %>
		    <%= text_field_tag(:routename)%>
		    <%= submit_tag 'create a Route', :class => "btn btn-primary" %>
		<% end %>
		<% @names.each do |name|%>
		<p><%=name%></p>
		<%end%>
		<% @location.each do |l|%>
		<p><%=l%></p>
		<%end%>
		<%@location_names.each do |locname|%>
		<%=locname.name%>
		<%end%>
		<%#= @current_user.getName%>
		<%= @current_route_id%>
		<%# #@locOfRoute.each do |loc|%>
		<p><%#=loc%></p>
		<%#end%>
		<%if @select=='1' %>
			<body onload="searchmap('<%=@filmloc%>',<%=@names%>)"></body>
		<%elsif @select=='2' %>
			<body onload="seelocation(<%=@location%>)"></body>
		<%end%>
		<ul >
    	<button onclick="drawRoute(<%=@loc%>)" class="btn btn-primary" >show route</button>
    	</ul>
    	
	</body>




